<div class="persistent-navbar">
  <h4><%= link_to "Wikinimous", articles_path , id: 'logo' %></h4>
  <%= link_to "New Article" , new_article_path, id: "create-article"  %>
</div>

<div class="container">
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2">
      <div >
        <% @articles.each do |article| %>
        <h2> <%= link_to article.title , article_path(article) %></h2>
        <p><%= article.content %></p>
        <% end %>
      </div>
    </div>
  </div>
</div>




